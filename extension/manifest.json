{
  "manifest_version": 2,
  "name": "Used AngularJS methods recorder",
  "description": "Records AngularJS methods used in react components",
  "version": "0.1",
  "permissions": [],
  "content_scripts": [
    {
      "js": ["getUsedNgMethods.js"],
      "matches": [
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ]
}
